{
  "name": "render-templates",
  "description": "Zero-dependency string template rendering.",
  "version": "1.0.0",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts"
      }
    }
  },
  "keywords": [
    "render",
    "object",
    "string",
    "template",
    "mustache"
  ],
  "scripts": {
    "build": "tsup",
    "prepublish": "pnpm build",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@underctrl/tsconfig": "^0.0.1",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1"
  }
}
